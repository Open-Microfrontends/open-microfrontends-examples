{
  "name": "@open-microfrontends/examples-browser-standalone-routing-host-application",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@open-microfrontends/types": "^1.0.0",
    "@open-microfrontends/generator": "^1.0.0",
    "typescript": "^5.9.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "@types/express": "^5.0.3",
    "page.js": "^4.13.3",
    "rollup": "^4.52.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-typescript": "^12.1.4",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4"
  },
  "scripts": {
    "generate": "omg ../microfrontends1.yaml src/_generated/mf1 -t startersBrowserStandalone && omg ../microfrontends2.yaml src/_generated/mf2 -t startersBrowserStandalone",
    "type-check": "tsc --noEmit",
    "build": "rollup --config rollup.config.ts --configPlugin typescript",
    "start": "npm run build && node --no-warnings=ExperimentalWarning --loader ts-node/esm/transpile-only src/server.ts"
  }
}
