{
  "name": "@open-microfrontends/examples-browser-standalone-routing-microfrontend2",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "http-server": "^14.1.1"
  },
  "devDependencies": {
    "@open-microfrontends/types": "file:../../../open-microfrontends/packages/types",
    "@open-microfrontends/open-microfrontends-generator": "file:../../../open-microfrontends-generator",
    "cpy-cli": "^6.0.0",
    "vue": "^3.5.22",
    "vue-tsc": "^3.1.0",
    "vue-router": "^4.5.1",
    "typescript": "^5.9.2",
    "tslib": "^2.8.1",
    "vite": "^7.1.7",
    "@vitejs/plugin-vue": "^6.0.1"
  },
  "scripts": {
    "generate": "omg ../microfrontends2.yaml src/_generated -t renderers",
    "type-check": "vue-tsc --noEmit",
    "build": "vite build && cpy ../microfrontends2.yaml dist --rename=microfrontends.yaml --flat",
    "dev": "vite serve",
    "start": "npm run build && http-server dist -p 7821 --cors -c-1"
  }
}
