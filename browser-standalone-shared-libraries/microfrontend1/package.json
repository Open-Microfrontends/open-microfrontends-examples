{
  "name": "@open-microfrontends/examples-browser-standalone-shared-libraries-microfrontend1",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "http-server": "^14.1.1"
  },
  "devDependencies": {
    "@open-microfrontends/types": "file:../../../open-microfrontends/packages/types",
    "@open-microfrontends/open-microfrontends-generator": "file:../../../open-microfrontends-generator",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "cpy-cli": "^6.0.0",
    "typescript": "^5.9.2",
    "ts-node": "^10.9.2",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "html-webpack-plugin": "^5.6.4",
    "ts-loader": "^9.5.4",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0"
  },
  "scripts": {
    "generate": "omg ../microfrontends1.yaml src/_generated -t renderers",
    "type-check": "tsc --noEmit",
    "build": "NODE_OPTIONS='--loader ts-node/esm' webpack --mode=production && cpy ../microfrontends1.yaml dist --rename=microfrontends.yaml --flat",
    "dev": "NODE_OPTIONS='--loader ts-node/esm' webpack serve --mode=development",
    "start": "npm run build && http-server dist -p 7810 --cors -c-1"
  }
